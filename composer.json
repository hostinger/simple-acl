{
    "name": "hostinger/acl",
    "description": "Simple ACL checker.",
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.0",
        "fabpot/php-cs-fixer": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Acl\\": "src"
        }
    },
    "scripts": {
        "fix": "php-cs-fixer fix ./src/  --level=psr2",
        "test": "phpunit"
    }
}
